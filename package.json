    "react-dom": ">=16.8.1"
  },
  "engines": {
    "node": "14.17.3",
    "npm": "6.14.13"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "cacheDirectories": []
}
