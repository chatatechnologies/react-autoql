{
  "name": "@chata-ai/core",
  "version": "0.0.6",
  "description": "Javascript widget library",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "watch 'npm run build' src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:staged": "jest --findRelatedTests",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "lint": "eslint '**/*.js'",
    "lint:fix": "prettier-eslint '**/*.js' --write",
    "storybook": "start-storybook -p 9001 -c storybook",
    "storybook:build": "build-storybook -c storybook",
    "predeploy:storybook": "npm run storybook:build",
    "deploy:storybook": "gh-pages -d storybook-static",
    "prepublish": "npm run build"
  },
  "style": "dist/styles.css",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "npm run test:staged",
      "git-add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chatatechnologies/chata-ai.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chatatechnologies/chata-ai/issues"
  },
  "homepage": "https://chatatechnologies.github.io/chata-ai/",
  "dependencies": {
    "axios": "^0.19.0",
    "papaparse": "^5.0.0",
    "prop-types": "^15.6.2",
    "rc-drawer": "^1.9.8",
    "react-autosuggest": "^9.4.3",
    "react-custom-scrollbars": "^4.2.1",
    "react-icons": "^3.7.0",
    "react-scroll-to-bottom": "^1.3.1",
    "react-speech-recognition": "^2.0.1",
    "react-tabulator": "^0.9.1",
    "react-tooltip": "^3.10.0",
    "uuid": "https://registry.npmjs.org/uuid/-/uuid-2.0.3.tgz"
  },
  "devDependencies": {
    "@storybook/addon-options": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "autoprefixer": "^9.6.0",
    "babel-cli": "https://registry.npmjs.org/babel-cli/-/babel-cli-6.26.0.tgz",
    "babel-core": "https://registry.npmjs.org/babel-core/-/babel-core-6.26.0.tgz",
    "babel-jest": "^23.6.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "https://registry.npmjs.org/babel-preset-env/-/babel-preset-env-1.6.1.tgz",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-react": "^6.24.1",
    "check-prop-types": "^1.1.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "gh-pages": "^2.0.1",
    "husky": "^2.3.0",
    "jest": "^23.6.0",
    "jest-css-modules": "^2.0.0",
    "lint-staged": "^8.1.7",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "rollup": "^1.12.4",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-svg": "^1.0.1",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-typescript2": "^0.21.1",
    "watch": "^1.0.2"
  },
  "peerDependencies": {
    "react": ">=16.2"
  },
  "engines": {
    "node": ">=8"
  },
  "resolutions": {
    "**/**/fsevents": "^1.2.9"
  }
}
