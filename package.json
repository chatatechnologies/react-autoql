{
  "name": "react-autoql",
  "version": "0.0.0-semantically-released",
  "description": "React Widget Library",
  "main": "dist/autoql.cjs.js",
  "module": "dist/autoql.esm.js",
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "export NODE_ENV=dev && watch 'npm run build' src",
    "packexample": "cd example/package && npm pack ../..",
    "test": "NODE_ENV=test && jest --silent=false",
    "test:watch": "jest --watch",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && npx make-coverage-badge --output-path ./public/badge.svg",
    "test:staged": "jest --findRelatedTests",
    "build": "rollup -c",
    "lint": "eslint '**/*.js'",
    "lint:fix": "prettier-eslint '**/*.js' --write",
    "prepublish": "npx rimraf dist && npm run build",
    "semantic-release": "semantic-release",
    "cz": "npx git-cz"
  },
  "style": "dist/autoql.esm.css",
  "husky": {
    "hooks": {
      "pre-commit": "npm run packexample && git add example/package/react-autoql-0.0.0-semantically-released.tgz",
      "pre-push": "npx jest --clearCache && npm run test"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run test:staged"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chatatechnologies/react-autoql.git"
  },
  "keywords": [],
  "author": "Nicole Moore",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chatatechnologies/react-autoql/issues"
  },
